{
    "users": [
      {
        "name": "${RABBITMQ_MQTT_USER}",
        "password": "${RABBITMQ_MQTT_PASSWORD}",
        "tags": "mqtt"
      },
      {
        "name": "${RABBITMQ_ADMIN_USER}",
        "password": "${RABBITMQ_ADMIN_PASSWORD}",
        "tags": "administrator"
      }
    ],
    "vhosts": [
      {
        "name": "/"
      },
      {
        "name": "/chirpstack"
      }
    ],
    "permissions": [
      {
        "user": "${RABBITMQ_MQTT_USER}",
        "vhost": "/chirpstack",
        "configure": ".*",
        "write": ".*",
        "read": ".*"
      }
    ]
  } 